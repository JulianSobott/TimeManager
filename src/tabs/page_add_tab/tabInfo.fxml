<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<fx:root
         stylesheets="@tabInfo.css"
         type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/10.0.2-internal"
         xmlns:fx="http://javafx.com/fxml/1">
    <VBox fx:id="container" styleClass="tabInfoContainer">
        <!--    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"/>-->
        <Label fx:id="lblName" minWidth="200" styleClass="lblName"/>
        <Label fx:id="lblShortDescription" styleClass="lblShortDescription"/>
        <Button text="Add" onMouseClicked="#installTab" styleClass="button-default"/>
    </VBox>
</fx:root>
